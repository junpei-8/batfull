---
import LayoutHeaderAvatar from '../header/LayoutHeaderAvatar.svelte';
---

<script>
  const win = window;
  const docEl = document.documentElement;
  const headerEl = document.getElementById('layout-header')!;
  let lastScrollTop = 0;
  let currentTranslateY = 0;

  function onScroll() {
    let scrollTop = win.scrollY || docEl.scrollTop;
    const headerHeight = headerEl.offsetHeight;

    let delta = scrollTop - lastScrollTop;
    currentTranslateY -= delta;

    // スクロール量が headerHeight を超えない
    if (currentTranslateY < -headerHeight) {
      currentTranslateY = -headerHeight;

      // Scroll 量が 0 以下にならないようにする
    } else if (currentTranslateY > 0) {
      currentTranslateY = 0;
    }

    headerEl.style.transform = `translateY(${currentTranslateY}px)`;

    lastScrollTop = scrollTop;
  }

  let ticking = false;
  window.addEventListener(
    'scroll',
    () => {
      if (ticking) return;
      requestAnimationFrame(() => (onScroll(), (ticking = false)));
      ticking = true;
    },
    { passive: true },
  );
</script>

